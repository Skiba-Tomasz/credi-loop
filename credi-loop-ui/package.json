{
  "name": "credi-loop-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "buildProd": "ng build --configuration=production",
    "clearS3": "aws s3 rm s3://crediloop.com --recursive",
    "uploadToS3": "aws s3 sync ./dist/credi-loop-ui/browser s3://crediloop.com",
    "cfInvalidate": "aws cloudfront create-invalidation --distribution-id E10XL9G5H8S7EE --paths '/'",
    "deploy": "yarn buildProd && yarn clearS3 && yarn uploadToS3 && yarn cfInvalidate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@forevolve/bootstrap-dark": "^4.0.2",
    "@metamask/detect-provider": "^2.0.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.1",
    "@ng-icons/bootstrap-icons": "^29.5.0",
    "@ng-icons/core": "^29.5.0",
    "@ng-icons/cryptocurrency-icons": "^29.5.0",
    "@ng-icons/iconoir": "^29.5.0",
    "@ng-icons/lucide": "^29.5.0",
    "@ng-icons/tdesign-icons": "^29.5.0",
    "@requestnetwork/request-client.js": "^0.49.0",
    "@requestnetwork/web3-signature": "^0.8.0",
    "apexcharts": "^3.53.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "4.2.0",
    "fuse.js": "^7.0.0",
    "ng-animate": "^2.0.1",
    "ng-apexcharts": "^1.11.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "process": "^0.11.10",
    "rxjs": "~7.8.0",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-devkit/build-angular": "^17.3.4",
    "@angular/cli": "^17.3.4",
    "@angular/compiler-cli": "^17.3.0",
    "@types/bootstrap": "^5.2.10",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}
